GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/EstimoteSDK" "${PODS_ROOT}/Headers/PaymentKit" "${PODS_ROOT}/Headers/Shimmer" "${PODS_ROOT}/Headers/Stripe" "${PODS_ROOT}/Headers/EstimoteSDK"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/EstimoteSDK" "$(PODS_ROOT)/EstimoteSDK/EstimoteSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/EstimoteSDK" -isystem "${PODS_ROOT}/Headers/PaymentKit" -isystem "${PODS_ROOT}/Headers/Shimmer" -isystem "${PODS_ROOT}/Headers/Stripe"
OTHER_LDFLAGS = -ObjC -lEstimoteSDK -framework CoreLocation -framework Foundation -framework MobileCoreServices -framework Security -framework SystemConfiguration -framework UIKit
PODS_ROOT = ${SRCROOT}/Pods